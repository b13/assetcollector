{
	"name": "b13/assetcollector",
	"type": "typo3-cms-extension",
	"description": "Add CSS and SVG files and strings as inline style tag/inline svg to the html code.",
	"license": "GPL-2.0-or-later",
	"config": {
		"vendor-dir": "build/vendor"
	},
	"require": {
		"php": "^7.2 || ^8.0",
		"typo3/cms-core": "^9.5 || ^10.0 || ^11.0",
		"typo3/cms-extbase": "^9.5 || ^10.0 || ^11.0",
		"typo3/cms-frontend": "^9.5 || ^10.0 || ^11.0"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "assetcollector"
		}
	},
	"autoload": {
		"psr-4": {
			"B13\\Assetcollector\\": "Classes/"
		}
	},
	"require-dev": {
		"typo3/coding-standards": "^0.2.0",
		"typo3/tailor": "^1.1"
	}
}
